<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="BaseAccountDao">
        <select id="searchNameAndPassword" parameterType="int" resultType="bean.EtAdmin">
            select name,password from et_admin where name =#[name ] and PASSWORD = #[PASSWORD]
        </select>
</mapper>
